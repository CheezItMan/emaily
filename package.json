{
  "name": "emailyServer",
  "version": "1.0.0",
  "description": "A sample Express app",
  "main": "index.js",
  "engines": {
    "node": "8.1.1",
    "npm": "5.0.3"
  },
  "scripts": {
    "test": "karma",
    "client": "npm run start --prefix client",
    "start": "node index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon index.js",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "webhook": "lt -p 5000 emaily-awesome.localtunnel.com"
  },
  "author": "Chris McAnally",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.2",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.2",
    "localtunnel": "^1.8.3",
    "mongoose": "^5.0.0-rc1",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "sendgrid": "^5.2.3",
    "stripe": "^5.4.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "nodemon": "^1.14.7"
  }
}
